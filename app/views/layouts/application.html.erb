<!DOCTYPE html>
<html>
<head>
  <title>Photoapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render "shared/navbar" %>
</head>
<body>
	<div class="container">
		<div class="row">
			<% if current_user %>
				<div class="col-sm-2 col-md-2 sidebar sidebar-left">
					<%= render "shared/sidebar_left" %>
				</div>
			<% end %>
			
			<div class="col-sm-10 col-sm-offset-2 col-md-10 col-md-offset-2 main" id="gallery">
				<%= yield %>
			</div>
		</div>
	</div>
</body>
</html>
